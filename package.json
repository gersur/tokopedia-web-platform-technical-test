{
  "name": "tokopedia-web-platform-technical-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/lab": "^5.0.0-alpha.65",
    "@mui/material": "^5.3.0",
    "@reduxjs/toolkit": "^1.7.1",
    "graphql": "^16.2.0",
    "mdi-material-ui": "^7.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "analyze": "source-map-explorer 'dist/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "barrel": "barrelsby --delete --singleQuotes -c barrelsby.json",
    "_download-schema": "npx apollo service:download --endpoint=https://graphql-pokeapi.vercel.app/api/graphql graphql-schema.json",
    "codegen": "npm run _download-schema && npx apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql && npm run barrel",
    "predeploy": "cross-env GH_PAGES=true webpack --mode=production",
    "deploy": "gh-pages -d dist"
  },
  "homepage": "https://gersur.github.io/tokopedia-web-platform-technical-test",
  "devDependencies": {
    "@iconify/react": "^3.1.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.18.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "barrelsby": "^2.3.2",
    "compression-webpack-plugin": "^9.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "node-sass": "^7.0.1",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^12.4.0",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  }
}
